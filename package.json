{
    "name": "ibmi-languages",
    "displayName": "IBMi Languages",
    "description": "Syntax highlighting for IBM i languages such as RPG, CL, DDS, MI, and RPGLE fixed/free.",
    "version": "0.6.14",
    "publisher": "barrettotte",
    "author": {
        "name": "Barrett Otte"
    },
    "engines": {
        "vscode": "^1.38.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "bnd",
                "aliases": [
                    "BND",
                    "Binder Language"
                ],
                "extensions": [
                    ".bnd"
                ],
                "configuration": "./configurations/bnd.language-configuration.json"
            },
            {
                "id": "cl",
                "aliases": [
                    "CL",
                    "CLLE",
                    "CLP",
                    "CLP38",
                    "Control Language"
                ],
                "extensions": [
                    ".cl",
                    ".clle",
                    ".clp",
                    ".clp38"
                ],
                "configuration": "./configurations/cl.language-configuration.json"
            },
            {
                "id": "cmd",
                "aliases": [
                    "CMD",
                    "Command Definition"
                ],
                "extensions": [
                    ".cmd"
                ],
                "configuration": "./configurations/cmd.language-configuration.json"
            },
            {
                "id": "dds.dspf",
                "aliases": [
                    "DSPF"
                ],
                "extensions": [
                    ".dspf",
                    ".dspf38"
                ],
                "configuration": "./configurations/dds.language-configuration.json"
            },
            {
                "id": "dds.icff",
                "aliases": [
                    "ICFF"
                ],
                "extensions": [
                    ".icff"
                ],
                "configuration": "./configurations/dds.language-configuration.json"
            },
            {
                "id": "dds.lf",
                "aliases": [
                    "LF"
                ],
                "extensions": [
                    ".lf",
                    ".lf38"
                ],
                "configuration": "./configurations/dds.language-configuration.json"
            },
            {
                "id": "dds.pf",
                "aliases": [
                    "DDS",
                    "PF"
                ],
                "extensions": [
                    ".dds",
                    ".pf",
                    ".pf38"
                ],
                "configuration": "./configurations/dds.language-configuration.json"
            },
            {
                "id": "dds.prtf",
                "aliases": [
                    "PRTF"
                ],
                "extensions": [
                    ".prtf",
                    ".prtf38"
                ],
                "configuration": "./configurations/dds.language-configuration.json"
            },
            {
                "id": "mi",
                "aliases": [
                    "MI"
                ],
                "extensions": [
                    ".MI"
                ],
                "configuration": "./configurations/mi.language-configuration.json"
            },
            {
                "id": "pnlgrp",
                "aliases": [
                    "PNLGRP",
                    "IBM UIM Panel Group Definition Language"
                ],
                "extensions": [
                    ".pnlgrp"
                ],
                "configuration": "./configurations/pnlgrp.language-configuration.json"
            },
            {
                "id": "rpg",
                "aliases": [
                    "RPG",
                    "SQLRPG",
                    "RPG/400",
                    "RPG38"
                ],
                "extensions": [
                    ".rpg",
                    ".rpg38",
                    ".sqlrpg"
                ],
                "configuration": "./configurations/rpg.language-configuration.json"
            },
            {
                "id": "rpgle",
                "aliases": [
                    "RPGLE",
                    "SQLRPGLE",
                    "RPGLEINC"
                ],
                "extensions": [
                    ".rpgle",
                    ".sqlrpgle",
                    ".rpgleinc"
                ],
                "configuration": "./configurations/rpgle.language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "bnd",
                "scopeName": "source.bnd",
                "path": "./syntaxes/bnd.tmLanguage.json"
            },
            {
                "language": "cl",
                "scopeName": "source.cl",
                "path": "./syntaxes/cl.tmLanguage.json"
            },
            {
                "language": "cmd",
                "scopeName": "source.cmd",
                "path": "./syntaxes/cmd.tmLanguage.json"
            },
            {
                "language": "dds.dspf",
                "scopeName": "source.dds.dspf",
                "path": "./syntaxes/dds.dspf.tmLanguage.json"
            },
            {
                "language": "dds.icff",
                "scopeName": "source.dds.icff",
                "path": "./syntaxes/dds.icff.tmLanguage.json"
            },
            {
                "language": "dds.lf",
                "scopeName": "source.dds.lf",
                "path": "./syntaxes/dds.lf.tmLanguage.json"
            },
            {
                "language": "dds.pf",
                "scopeName": "source.dds.pf",
                "path": "./syntaxes/dds.pf.tmLanguage.json"
            },
            {
                "language": "dds.prtf",
                "scopeName": "source.dds.prtf",
                "path": "./syntaxes/dds.prtf.tmLanguage.json"
            },
            {
                "language": "mi",
                "scopeName": "source.mi",
                "path": "./syntaxes/mi.tmLanguage.json"
            },
            {
                "language": "pnlgrp",
                "scopeName": "source.pnlgrp",
                "path": "./syntaxes/pnlgrp.tmLanguage.json"
            },
            {
                "language": "rpg",
                "scopeName": "source.rpg",
                "path": "./syntaxes/rpg.tmLanguage.json"
            },
            {
                "language": "rpgle",
                "scopeName": "source.rpgle",
                "path": "./syntaxes/rpgle.tmLanguage.json"
            }
        ]
    },
    "icon": "images/icon.png",
    "devDependencies": {
        "js-yaml": "^3.13.1",
        "semver": "^7.3.5"
    },
    "keywords": [
        "RPG",
        "RPGLE",
        "IBMi",
        "AS/400",
        "SQLRPG",
        "SQLRPGLE",
        "CL",
        "CONTROL LANGUAGE",
        "DDS",
        "DSPF",
        "MI",
        "MACHINE INTERFACE",
        "ISERIES",
        "BINDER LANGUAGE"
    ],
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/barrettotte/vscode-ibmi-languages"
    }
}
